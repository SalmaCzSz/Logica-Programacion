Algoritmo OperadoresAritmeticos
	sumar <- 3+4
	Escribir 'Suma: ', sumar
	resta <- 6-2
	Escribir 'Resta: ', resta
	multiplicacion <- 12*5
	Escribir 'Multiplicacion: ', multiplicacion
	division <- resta/sumar
	Escribir 'División: ', division
	potencia <- 2^2
	Escribir 'Potencia: ', potencia
	modulo <- 9 MOD 3
	Escribir 'Modulo: ', modulo
FinAlgoritmo
